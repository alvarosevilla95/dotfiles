{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    },
    "java.import.gradle.enabled": true,
    "java.trace.server": "verbose",
    "java.import.gradle.wrapper.enabled": false,
    "java.completion.enabled": true,
    "coc.preferences.extensionUpdateCheck": "never",
    "coc.preferences.hoverTarget": "float"
}
